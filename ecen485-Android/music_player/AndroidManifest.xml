<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uriahnewkirk.funky_beats">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/black_t">
        <activity android:name=".beats">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".githubWeb"
            android:parentActivityName=".beats_contact" >

        </activity>
        <activity
            android:name=".beats_contact"
            android:parentActivityName=".beats" >


        </activity>
        <activity
            android:name=".beats_settings"
            android:parentActivityName=".beats" >

        </activity>
        <activity
            android:name=".EditPreferences"
            android:parentActivityName=".beats" >

        </activity>
    </application>

</manifest>